{
  "extends": "eslint:recommended",
  "env": {
    "browser": true
  },
  "globals": {},
  "rules": {
    "eqeqeq": [
      2,
      "smart"
    ],
    "curly": [
      2,
      "multi-line"
    ],
    "guard-for-in": 2,
    "no-alert": 2,
    "block-scoped-var": 2,
    "complexity": [
      0,
      11
    ],
    "consistent-return": 2,
    "default-case": 2,
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "dot-location": 0,
    "no-caller": 2,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 0,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-console": 0
  }
}
